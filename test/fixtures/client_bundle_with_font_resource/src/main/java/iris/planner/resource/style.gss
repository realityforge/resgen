@def GLYPHICONS resourceUrl("glyphiconsHalflingsRegular");

@font-face {
  font-family: 'Glyphicons Halflings';
  src: GLYPHICONS format('woff');
}

.glyphicon {
  position: relative;
  top: 1px;
  display: inline-block;
  font-family: 'Glyphicons Halflings';
  -webkit-font-smoothing: antialiased;
  font-style: normal;
  font-weight: normal;
  line-height: 1;
}

.glyphicon-plus:before {
  content: "\2b";
}
